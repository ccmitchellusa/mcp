{
  "mcpServers": {
    "ibmcloud": {
      "command": "ibmcloud",
      "args": [
        "--mcp-transport",
        "stdio",
        "--mcp-tools",
        "target,catalog_locations,catalog_service,catalog_service-marketplace,catalog_pricing,catalog_search"
      ],
      "env": {
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    }
  }
}